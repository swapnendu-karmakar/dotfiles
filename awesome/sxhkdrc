#reload sxhkd
ctrl + Escape
	pkill -USR1 -x sxhkd

#reload sxhkd
super + p
	dmenu_run

#terminal
super + shift + Return
        alacritty

#arcolinux-logout
super + shift + q
	arcolinux-logout

#filemanager
super + alt + f
	thunar

#firefox
super + alt + b
	firefox

#nitrogen
super + alt + w
	nitrogen --set-scaled --random    
